    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "clean-webpack-plugin": "^0.1.17",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.7",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
