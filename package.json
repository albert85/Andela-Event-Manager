{
  "name": "andela-event-manager",
  "version": "1.0.0",
  "description": "[![Build Status](https://travis-ci.org/albert85/Andela-Event-Manager.svg?branch=develop)](https://travis-ci.org/albert85/Andela-Event-Manager)",
  "main": "./server/app.js",
  "devDependencies": {},
  "scripts": {
    "babel-node": "babel-node --presets=latest",
    "start": "babel-watch ./client/clientServer/prodServer.js || true",
    "start:dev": "nodemon --exec npm run babel-node server/app.js",
    "js": "npm run db:migrate && nyc --reporter=html --reporter=text mocha --compilers js:babel-register --require babel-polyfill --recursive server/test --timeout 15000 --exit",
    "test": "npm run db:migrate && nyc --reporter=html --reporter=text mocha --compilers js:babel-core/register --require babel-polyfill --recursive server/test/test/ --timeout 15000 --exit",
    "test:client": "jest ./client --coverage",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "db:migrate": "sequelize --config ./server/config/config.json --migrations-path ./server/migrations db:migrate:undo:all && sequelize --config ./server/config/config.json --migrations-path ./server/migrations db:migrate",
    "prod": "webpack-dev-server --config webpack.prod.js",
    "web:webpack": "webpack --watch --config webpack.dev.js",
    "product2": "babel-node -- ./client/clientServer/prodServer.js",
    "postinstall": "webpack -p --config webpack.prod.js",
    "e2eTest": "./node_modules/.bin/nightwatch"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/setupFile.js"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "client/src/**/*.{js,jsx}"
    ],
    "moduleNameMapper": {
      "^.*[.](jpe?g|gif|png|less|css|scss)$": "<rootDir>/SassStub.js"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/client/__test__/e2e_test/"
    ],
    "testEnvironment": "jsdom",
    "globals": {
      "window": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/albert85/Andela-Event-Manager.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/albert85/Andela-Event-Manager/issues"
  },
  "homepage": "https://github.com/albert85/Andela-Event-Manager#readme",
